            <!-- active  -->
            <style>
                .switch {
                    position: relative;
                    display: inline-block;
                    width: 45px;
                    height: 22px;
                }
                .switch input {
                    opacity: 0;
                    width: 0;
                    height: 0;
                }
                .slider {
                    position: absolute;
                    cursor: pointer;
                    top: 0;
                    left: 0;
                    right: 0;
                    bottom: 0;
                    background-color: rgb(230, 109, 109);
                    -webkit-transition: .4s;
                    transition: .4s;
                }
                .slider:before {
                    position: absolute;
                    content: "";
                    height: 14px;
                    width: 14px;
                    left: 4px;
                    bottom: 4px;
                    background-color: rgb(233, 241, 231);
                    -webkit-transition: .4s;
                    transition: .4s;
                }
                input:checked+.slider {
                    background-color: #1c6322;;
                }
                input:focus+.slider {
                    box-shadow: 0 0 1px #1c6322;;
                }
                input:checked+.slider:before {
                    -webkit-transform: translateX(26px);
                    -ms-transform: translateX(26px);
                    transform: translateX(26px);
                }
                .slider.round {
                    border-radius: 34px;
                }
                .slider.round:before {
                    border-radius: 50%;
				}
            </style>
